    <!-- https://hack.cybersystems.engineer/memex/ontologies/SeamlessDigitalEngineering#Trade_Study_Process -->

    <owl:Class rdf:about="https://hack.cybersystems.engineer/memex/ontologies/SeamlessDigitalEngineering#Trade_Study_Process">
