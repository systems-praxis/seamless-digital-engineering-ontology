        <cco2:ont00001754 xml:lang="en">ISO/IEC 25010:2023</cco2:ont00001754>
    </owl:Class>
    


    <!-- https://hack.cybersystems.engineer/memex/ontologies/SeamlessDigitalEngineering#Product_Safe_Integration -->

