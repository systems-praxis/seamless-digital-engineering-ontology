    <!-- https://hack.cybersystems.engineer/memex/ontologies/SeamlessDigitalEngineering#Capability -->

    <owl:Class rdf:about="https://hack.cybersystems.engineer/memex/ontologies/SeamlessDigitalEngineering#Capability">
