    <!-- https://hack.cybersystems.engineer/memex/ontologies/SeamlessDigitalEngineering#Authoritative_Knowledge -->

    <owl:Class rdf:about="https://hack.cybersystems.engineer/memex/ontologies/SeamlessDigitalEngineering#Authoritative_Knowledge">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
