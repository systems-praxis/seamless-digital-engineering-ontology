            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://hack.cybersystems.engineer/memex/ontologies/SeamlessDigitalEngineering#Decision_Management_Process"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://www.commoncoreontologies.org/ont00001986"/>
