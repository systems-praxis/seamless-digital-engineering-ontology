    <!-- https://hack.cybersystems.engineer/memex/ontologies/SeamlessDigitalEngineering#Acceptability -->

    <owl:Class rdf:about="https://hack.cybersystems.engineer/memex/ontologies/SeamlessDigitalEngineering#Acceptability">
        <rdfs:subClassOf rdf:resource="https://hack.cybersystems.engineer/memex/ontologies/SeamlessDigitalEngineering#Product_Quality-in-Use"/>
        <rdfs:label xml:lang="en">Acceptability</rdfs:label>
        <skos:definition xml:lang="en">extent to which a human response is favourable when accepting or installing a Product, System or Service software tool designed to perform some frequently used function</skos:definition>
        <cco2:ont00001754 xml:lang="en">ISO/IEC 25019:2023(en)
Systems and software engineering — Systems and software Quality Requirements and Evaluation (SQuaRE) — Quality-in-use model</cco2:ont00001754>
    </owl:Class>
    


    <!-- https://hack.cybersystems.engineer/memex/ontologies/SeamlessDigitalEngineering#Acceptance_Report -->

