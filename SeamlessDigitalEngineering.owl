                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://www.commoncoreontologies.org/ont00001857"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="https://hack.cybersystems.engineer/memex/ontologies/SeamlessDigitalEngineering#Lifecycle"/>
