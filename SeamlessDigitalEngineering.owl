    <owl:Class rdf:about="https://hack.cybersystems.engineer/memex/ontologies/SeamlessDigitalEngineering#Narrative_Introduction_Report">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://www.commoncoreontologies.org/ont00000624"/>
